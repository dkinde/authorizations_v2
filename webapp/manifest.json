{
  "_version": "1.49.0",
  "sap.app": {
    "id": "authorization",
    "type": "application",
    "i18n": {
      "bundleUrl": "i18n/i18n.properties",
      "supportedLocales": [
        "en",
        "de"
      ],
      "fallbackLocale": "de"
    },
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.9.2",
      "toolsId": "140dda3d-eded-4201-be1a-6421e05d6308"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata4/sap/zui_c_auth/srvd/sap/zui_c_auth/0001/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.111.4",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "authorization.i18n.i18n",
          "supportedLocales": [
            "en",
            "de"
          ],
          "fallbackLocale": "de"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "authorization.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false,
        "transition": "slide",
        "bypassed": {
          "target": [
            "TargetHome"
          ]
        }
      },
      "routes": [
        {
          "name": "RouteHome",
          "pattern": ":?query:",
          "target": [
            "TargetHome"
          ]
        },
        {
          "name": "RouteBasisKonfig",
          "pattern": "BasisKonfig",
          "target": [
            "TargetBasisKonfig"
          ]
        },
        {
          "name": "RouteFunktion",
          "pattern": "Funktion",
          "target": [
            "TargetFunktion"
          ]
        },
        {
          "name": "RouteWizard",
          "pattern": "Wizard",
          "target": [
            "TargetWizard"
          ]
        },
        {
          "name": "RouteKontroll",
          "pattern": "Kontroll",
          "target": [
            "TargetKontroll"
          ]
        },
        {
          "name": "RoutePersPlan",
          "pattern": "PERID_PLAPHA",
          "target": [
            "TargetPERID_PLAPHA"
          ]
        },
        {
          "name": "RouteAUDMART",
          "pattern": "BasisKonfig/AUDMART",
          "target": [
            "TargetAUDMART"
          ]
        },
        {
          "name": "RouteAUMPVDM",
          "pattern": "BasisKonfig/AUMPVDM",
          "target": [
            "TargetAUMPVDM"
          ]
        },
        {
          "name": "RouteHAUPARZL",
          "pattern": "BasisKonfig/HAUPARZL",
          "target": [
            "TargetHAUPARZL"
          ]
        },
        {
          "name": "RouteHAUPF001",
          "pattern": "Funktion/HAUPF001",
          "target": [
            "TargetHAUPF001"
          ]
        },
        {
          "name": "RouteMasterPersFKT",
          "pattern": ":layout:",
          "target": [
            "TargetMasterPersFKT"
          ]
        },
        {
          "name": "RouteDetailPersFKT",
          "pattern": "Funktion/HAUPF001/{funktion}/{layout}",
          "target": [
            "TargetDetailPersFKT",
            "TargetHAUPF001"
          ]
        },
        {
          "name": "RouteHAUFW001",
          "pattern": "Funktion/HAUFW001",
          "target": [
            "TargetHAUFW001"
          ]
        }
      ],
      "targets": {
        "TargetHome": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Home",
          "viewName": "Home",
          "viewLevel": 0
        },
        "TargetBasisKonfig": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "BasisKonfig",
          "viewName": "BasisKonfig",
          "viewLevel": 1
        },
        "TargetFunktion": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "Funktion",
          "viewName": "Funktion",
          "viewLevel": 1
        },
        "TargetWizard": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "Wizard",
          "viewName": "Wizard",
          "viewLevel": 1
        },
        "TargetKontroll": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "HAUKB001",
          "viewName": "HAUKB001",
          "viewLevel": 1
        },
        "TargetPERID_PLAPHA": {
          "viewType": "XML",
          "viewId": "PERID_PLAPHA",
          "viewName": "PERID_PLAPHA",
          "viewLevel": 1,
          "transition": "slide"
        },
        "TargetAUDMART": {
          "viewType": "XML",
          "viewId": "AUDMART",
          "viewName": "AUDMART",
          "viewLevel": 2,
          "transition": "slide"
        },
        "TargetAUMPVDM": {
          "viewType": "XML",
          "viewId": "AUMPVDM",
          "viewName": "AUMPVDM",
          "viewLevel": 2,
          "transition": "slide"
        },
        "TargetHAUPARZL": {
          "viewType": "XML",
          "viewId": "HAUPARZL",
          "viewName": "HAUPARZL",
          "viewLevel": 2,
          "transition": "slide"
        },
        "TargetHAUPF001": {
          "viewType": "XML",
          "viewId": "HAUPF001",
          "viewName": "HAUPF001",
          "viewLevel": 2,
          "transition": "slide"
        },
        "TargetHAUFW001": {
          "viewType": "XML",
          "viewId": "HAUFW001",
          "viewName": "HAUFW001",
          "viewLevel": 2,
          "transition": "slide"
        },
        "TargetMasterPersFKT": {
          "viewType": "XML",
          "viewId": "MasterPersFKT",
          "viewName": "MasterPersFKT",
          "viewLevel": 2,
          "transition": "slide"
        },
        "TargetDetailPersFKT": {
          "viewType": "XML",
          "viewId": "DetailPersFKT",
          "viewName": "DetailPersFKT",
          "viewLevel": 2,
          "transition": "slide"
        }
      }
    },
    "rootView": {
      "viewName": "authorization.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    },
    "handleValidation": true
  }
}