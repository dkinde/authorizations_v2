<core:FragmentDefinition xmlns="sap.m" 
                         xmlns:core="sap.ui.core"    						 						 				 
                         width="100%">			
		<SelectDialog	id="selectDialogPersonal"						
						noDataText="{i18n>noDataTextPersonal}"
						title="{i18n>selectDialogTitlePersonal}"
						searchPlaceholder="{i18n>searchPlaceholderPersonal}"
						search=".onSearchPersonal"
						confirm=".multiInputPersTokenUpdate"
						cancel=".onValueHelpDialogClose"
						contentWidth="400px"						
						growingThreshold="20"						
						multiSelect="true"
						showClearButton="true"												
						items="{
							path: '/PIPEPERID',
							sorter: {
								path: 'personalnummer',
								descending: false
							}
						}">
			<StandardListItem							
							title="{personalnummer}"
							description="{Txtmd}"							
							iconDensityAware="false"
							iconInset="false"
							type="Active" />
		</SelectDialog>			
</core:FragmentDefinition>