<core:FragmentDefinition xmlns="sap.m" 
                         xmlns:core="sap.ui.core"    						 						 				 
                         width="100%">			
		<SelectDialog	id="selectDialogPersonal"
						noDataText="Keinen Personalnummer gefunden"
						title="Personalnummer auswÃ¤hlen"
						search=".onSearchPersonal"
						searchPlaceholder="Personalnummer suchen"
						confirm=".multiInputPersTokenUpdate"
						cancel=".onValueHelpDialogClose"
						contentWidth="300px"						
						growingThreshold="20"						
						multiSelect="true"
						showClearButton="true"												
						items="{
							path: '/PIPEPERID',
							sorter: {
								path: 'personalnummer',
								descending: false
							}
						}">
			<StandardListItem							
							title="{personalnummer}"
							description="{Txtmd}"							
							iconDensityAware="false"
							iconInset="false"
							type="Active" />
		</SelectDialog>			
</core:FragmentDefinition>